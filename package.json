{
	"name": "livecode",
	"displayName": "LiveCode",
	"description": "Syntax highlighting for livecode language",
	"version": "0.0.1",
	"publisher": "FerrusLogic",
	"author": "FerrusLogic",
	"icon": "images/LClogo.svg",
	"engines": {
		"vscode": "^1.24.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "livecodescript",
				"aliases": [
					"livecodescript"
				],
				"extensions": [
					".livecodescript"
				],
				"configuration": "./language-configuration.json"
			},
			{
				"id": "lcb",
				"aliases": [
					"lcb"
				],
				"extensions": [
					".lcb"
				],
				"configuration": "./lcb-language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "livecodescript",
				"scopeName": "source.livecodescript",
				"path": "./syntaxes/livecodescript.tmLanguage.json"
			},
			{
				"language": "lcb",
				"scopeName": "source.lcb",
				"path": "./syntaxes/lcb.tmLanguage.json"
			}
		],

		"snippets": [
			{
				"language": "livecodescript",
				"path": "./snippets/livecode.code-snippets"
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ferruslogic/livecodescript-language.git"
	},
	"keywords": [
		"livecode",
		"livecode-builder",
		"lcb",
		"livecodescript-language",
		"vscode",
		"vscode-extension",
		"vscode-language"
	],

	
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Ferruslogic/livecode-syntax-highlighting/issues"
	},
	"homepage": "https://github.com/Ferruslogic/livecode-syntax-highlighting#readme",

}